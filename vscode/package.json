{
  "name": "lasecplot",
  "description": "Telemetry viewer.",
  "version": "1.4.1",
  "publisher": "josuemoraisgh",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/josuemoraisgh/LasecPlot"
  },
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:lasecplot.start"
  ],
  "main": "./out/extension.js",
  "icon": "images/logo-color.png",
  "contributes": {
    "commands": [
      {
        "command": "lasecplot.start",
        "title": "Start LasecPlot session",
        "category": "lasecplot",
        "icon": {
          "light": "images/logo-color.svg",
          "dark": "images/logo-color.svg"
        }
      }
    ],
	"configuration": {
		"title": "LasecPlot",
		"properties": {
			"lasecplot.udpPort": {
				"type": "number",
				"default": 47269,
				"description": "Porta UDP local (LP) para dados."
			}
		}
	}
},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts",
    "watch": "tsc -w -p ./",
    "test": "node ./out/test/runTests.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "serialport": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/vscode": "^1.94.0",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "multicast-dns": "^7.2.5",
    "typescript": "^5.6.3",
    "@vscode/test-electron": "^2.4.0",
    "@vscode/vsce": "^3.1.0"
  },
  "__metadata": {
    "installedTimestamp": 1762004166236,
    "targetPlatform": "undefined",
    "size": 25843053
  },
  "files": [
    "out",
    "media",
    "images",
    "README.md",
    "LICENSE.txt",
    "package.json"
  ]
}