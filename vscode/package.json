{
  "name": "lasecplot",
  "displayName": "LasecPlot",
  "description": "LasecPlot â€“ UDP telemetry viewer.",
  "version": "1.3.8",
  "publisher": "josuemoraisgh",
  "license": "MIT",
  "repository": { "type": "git", "url": "https://github.com/josuemoraisgh/LasecPlot" },
  "engines": { "vscode": "^1.90.0" },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:lasecplot.start",
    "onStartupFinished",
    "onWebviewPanel:lasecplot"
  ],
  "main": "./out/extension.js",
  "icon": "images/logo-color.png",
  "contributes": {
    "commands": [
      {
        "command": "lasecplot.start",
        "title": "Start LasecPlot",
        "category": "LasecPlot",
        "icon": { "light": "images/logo-color.svg", "dark": "images/logo-color.svg" }
      }
    ],
    "configuration": {
      "title": "LasecPlot",
      "properties": {
        "lasecplot.udpAddress": { "type": "string", "default": "127.0.0.1" },
        "lasecplot.udpPort":    { "type": "number", "default": 47269 },
        "lasecplot.remoteAddress": { "type": "string", "default": "127.0.0.1" },
        "lasecplot.cmdUdpPort": { "type": "number", "default": 47268 }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "lint": "eslint . --ext .ts"
  },
  "files": [
    "out/**",
    "media/**",
    "images/**",
    "package.json",
    "README.md",
    "LICENSE.txt"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.4.0"
  }
}